//Program to print ASCII code along with the character
#include <stdio.h>

void main()
{
    int i;
    /* Print ASCII values from 32 to 127*/
    for(i=32; i<=127; i++) 
    {
        if(i==127)
            printf("ASCII value of character DEL = 127");
        else
            printf("ASCII value of character %c = %d\n", i, i);
    }
}
